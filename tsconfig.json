{
	"display": "Nova",
	"compilerOptions": {
		"target": "es6",
		"module": "ES2020",
		"moduleResolution": "node",
		"newLine": "lf",
		"strict": true,
		"noEmitOnError": true,
		"skipLibCheck": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"]
}
